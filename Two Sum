class Solution1:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        set=[]
        n=len(nums)
        for i in range (0,n):
            for j in range (i+1,n):
                if nums[i] + nums[j] == target:
                        set.append(i)
                        set.append(j)
                        return set

class Solution2:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        n=len(nums)
        result=[]
        for i in range(n):
            needed=target-nums[i]
            for j in range(n):
                if i != j and nums[j]==needed:
                    result.append(i)
                    result.append(j)
                    return result

class Solution3:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        seen={}
        for i in range(len(nums)):
            j=target-nums[i]
            if j in seen:
                return[seen[j],i]
            seen[nums[i]]=i


