class Solution1:
     def hasDuplicate(self, nums: List[int]) -> bool:
         n=len(nums)
         for i in nums:
             count=0
             for a in range(0,n):
                 if(nums[a]==i):
                     count+=1
             if count > 1:
                 return True
  
         return False

class Solution2:
     def hasDuplicate(self, nums: List[int]) -> bool:
         return len(nums) != len(set(nums))

class Solution3:
    def hasDuplicate(self, nums: List[int]) -> bool:
        seen = set()
        for n in nums:
            if n in seen:
                return True   # duplicate found early
            seen.add(n)
        return False
